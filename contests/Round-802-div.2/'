#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define sz(c) (int)c.size()
#define all(c) c.begin(),c.end()

// Try to get to 9999 or 1001 if leading 9
// Has to be the same length

void solve() {
	ll n;
	string s;
	cin >> n >> s;
	vector<int> number(n+1,0), takeAway(n+1)
	for (int i = 0; i < n; i++) {
		number[i+1] = s[i]-48;
	}
}

int main(void) {
	ios::sync_with_stdio(0);
	cin.tie(0);
	
	cout << "\n";
	int t;
	cin >> t;
	for (int i = 1; i <= t; ++i)
		solve();
}

